    <path refid="library.jsr305-2.0.1.classpath"/>
    <path refid="library.android-android-24.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.citypicker">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.citypicker">
    <patternset refid="excluded.from.module.citypicker"/>
  </patternset>
  
  <path id="citypicker.module.sourcepath">
    <dirset dir="${module.citypicker.basedir}">
      <include name="build/generated/source/r/debug"/>
      <include name="build/generated/source/aidl/debug"/>
      <include name="build/generated/source/buildConfig/debug"/>
      <include name="build/generated/source/rs/debug"/>
      <include name="build/generated/res/rs/debug"/>
      <include name="build/generated/res/resValues/debug"/>
      <include name="src/main/res"/>
      <include name="src/main/assets"/>
      <include name="src/main/java"/>
    </dirset>
  </path>
  
  <path id="citypicker.module.test.sourcepath">
    <dirset dir="${module.citypicker.basedir}">
      <include name="build/generated/source/r/androidTest/debug"/>
      <include name="build/generated/source/aidl/androidTest/debug"/>
      <include name="build/generated/source/buildConfig/androidTest/debug"/>
      <include name="build/generated/source/rs/androidTest/debug"/>
      <include name="build/generated/res/rs/androidTest/debug"/>
      <include name="build/generated/res/resValues/androidTest/debug"/>
      <include name="src/test/java"/>
      <include name="src/androidTest/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.citypicker" depends="compile.module.citypicker.production,compile.module.citypicker.tests" description="Compile module citypicker"/>
  
  <target name="compile.module.citypicker.production" depends="register.custom.compilers" description="Compile module citypicker; production classes">
    <mkdir dir="${citypicker.output.dir}"/>
    <javac2 destdir="${citypicker.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.citypicker}/javac">
      <compilerarg line="${compiler.args.citypicker}"/>
      <bootclasspath refid="citypicker.module.bootclasspath"/>
      <classpath refid="citypicker.module.production.classpath"/>
      <src refid="citypicker.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.citypicker"/>
    </javac2>
    
    <copy todir="${citypicker.output.dir}">
      <fileset dir="${module.citypicker.basedir}/build/generated/source/r/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/aidl/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/buildConfig/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/res/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/res/resValues/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/src/main/res">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/src/main/assets">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.citypicker.tests" depends="register.custom.compilers,compile.module.citypicker.production" description="compile module citypicker; test classes" unless="skip.tests">
    <mkdir dir="${citypicker.testoutput.dir}"/>
    <javac2 destdir="${citypicker.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.citypicker}/javac">
      <compilerarg line="${compiler.args.citypicker}"/>
      <bootclasspath refid="citypicker.module.bootclasspath"/>
      <classpath refid="citypicker.module.classpath"/>
      <src refid="citypicker.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.citypicker"/>
    </javac2>
    
    <copy todir="${citypicker.testoutput.dir}">
      <fileset dir="${module.citypicker.basedir}/build/generated/source/r/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/aidl/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/buildConfig/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/source/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/res/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/build/generated/res/resValues/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.citypicker.basedir}/src/androidTest/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.citypicker" description="cleanup module">
    <delete dir="${citypicker.output.dir}"/>
    <delete dir="${citypicker.testoutput.dir}"/>
  </target>
</project>