  <path id="citypicker.runtime.production.module.classpath">
    <pathelement location="${citypicker.output.dir}"/>
    <path refid="library.support-v4-24.2.1.classpath"/>
    <path refid="library.support-compat-24.2.1.classpath"/>
    <path refid="library.support-media-compat-24.2.1.classpath"/>
    <path refid="library.animated-vector-drawable-24.2.1.classpath"/>
    <path refid="library.support-fragment-24.2.1.classpath"/>
    <path refid="library.support-core-ui-24.2.1.classpath"/>
