    <path refid="library.javax.inject-1.classpath"/>
    <path refid="library.hamcrest-integration-1.3.classpath"/>
    <path refid="library.support-annotations-24.2.1.classpath"/>
    <path refid="library.amap_location_v3.3.0_20170118.classpath"/>
    <path refid="library.javawriter-2.1.1.classpath"/>
    <path refid="library.hamcrest-core-1.3.classpath"/>
    <path refid="library.junit-4.12.classpath"/>
    <path refid="library.jsr305-2.0.1.classpath"/>
    <path refid="library.android-android-24.classpath"/>
  </path>
  
  <path id="citypicker.runtime.module.classpath">
    <pathelement location="${citypicker.testoutput.dir}"/>
    <pathelement location="${citypicker.output.dir}"/>
    <path refid="library.support-v4-24.2.1.classpath"/>
    <path refid="library.support-compat-24.2.1.classpath"/>
    <path refid="library.support-media-compat-24.2.1.classpath"/>
    <path refid="library.animated-vector-drawable-24.2.1.classpath"/>
    <path refid="library.support-fragment-24.2.1.classpath"/>
    <path refid="library.espresso-core-2.2.2.classpath"/>
